<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane id="AnchorPane" prefHeight="710.0" prefWidth="1101.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projectmedia.PlaylistController">
    <children>
        <BorderPane fx:id="newscreen" prefHeight="585.0" prefWidth="1093.0">
            <center>
                <StackPane prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <MediaView fx:id="mv" fitHeight="200.0" fitWidth="200.0" />
                    </children>
                </StackPane>
            </center>
        </BorderPane>
        <HBox layoutX="180.0" layoutY="587.0" prefHeight="109.0" prefWidth="816.0">
            <children>
                <Button fx:id="play" mnemonicParsing="false" onAction="#play" text="play" />
                <Button fx:id="stop" mnemonicParsing="false" onAction="#stop" text="stop" />
                <Button mnemonicParsing="false" onAction="#gotoplaylist" text="go to  playlist" />
            </children>
        </HBox>
    </children>
</AnchorPane>
